# LinkedList
class Node {
int i;
Node next;
}

class Linked {

 public Node insertFront(int i , Node node){  
            Node a = getNewNode(i);
            a.next = node;
            return a;
        }

// print 
public void printList(Node node) {
if(node == null )
{
return;
}
System.out.println(node.i+ " ");
printList(node.next);
}
private Node getNewNode(int i) {
Node a = new Node();
a.i = i;
a.next = null;
return a;
}

// Insert
public Node insert(int i , Node node) {
if(node == null) {
return getNewNode(i);
}


java -cp /tmp/eIdduxZsuN Linked
99 37 
else {
node.next = insert(i,node.next);

}
return node;
}

// delete frant
public Node deleteF(Node node){
if(node == null ){
return null;
}
return node.next;
}

//main 
public static void main(String[] args){
Node root = null;
Linked a = new Linked();
root = a.deleteF(root);
root = a.insert(12 , root);
root = a.insert(99 , root);
root = a.insert(37 , root);

root = a.deleteF(root);
a.printList(root);
System.out.println();

}
}
